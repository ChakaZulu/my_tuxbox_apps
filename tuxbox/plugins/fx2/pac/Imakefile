all:: pacman.so pac

PREF=powerpc-linux-
CC=$(PREF)gcc
STRIP=$(PREF)strip
CDEBUGFLAGS=-g -O2 -Wall -pedantic
INCLUDES=-I. -I../lib
OBJS=maze.o

SingleProgramTarget(pac,main.o $(OBJS),,-L../lib -lfx2)

maze.o: pics.h text.h

pacman.so: somain.o $(OBJS)
	$(CC) -o pacman.so -shared -Wl,-soname,pacman.so somain.o $(OBJS)
	$(STRIP) pacman.so

clean::
	rm -f pacman.so
