function update()
{
	data.location = "/data";
	setTimeout("update()", 5000);
}
function go(targetURL)
{
	var options = new Array(":input-repeat");
	document.vlc.stop();
	document.vlc.addTarget(targetURL, options, 4+8, -666);
}
function play()
{
	if (data.vlcparms)
		go(data.vlcparms);
	else
		setTimeout("play()", 500);
}
