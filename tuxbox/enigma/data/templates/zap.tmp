<script language="javascript" type="text/javascript" src="zap.js"></script>
<script language="javascript" type="text/javascript" src="zapdata.js"></script>
<form name="channelselector">
	<table border=0 cellpadding=0 cellspacing=0>
		<tr>
			<td>
				<select style="width: 200px; font-weight: bold;" onChange=bouquetChange() name="bouquet" size=30>
				</select>
			</td>
			<td>
				<select style="width: 500px; font-weight: bold;" onChange=channelChange() name="channel" size=30>
				</select>
			</td>
		</tr>
	</table>
	<br>
</form>
<button name="mepg" style="width: 100px;" value="MEPG" onClick=mepg()>Multi-EPG</button>
&nbsp;&nbsp;
<button name="epg" style="width: 100px;" value="EPG" onClick=epg()>EPG</button>
