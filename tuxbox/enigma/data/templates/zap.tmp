<html>
	<head>
		<link rel="stylesheet" type="text/css" href="webif.css">
		<script language="javascript" type="text/javascript" src="window.js"></script>
		<script language="javascript" type="text/javascript" src="index.js"></script>
		<script language="javascript" type="text/javascript" src="zap.js"></script>
		<script language="javascript" type="text/javascript">#ZAPDATA#</script>
	</head>
	<body onLoad="init()" style="padding: 10px; background-color: #F4F4F4" leftmargin=0 topmargin=0 marginwidth=0 marginheight=0 id="contentcell">
		<form name="channelselector">
			<table border=0 cellpadding=0 cellspacing=0>
				<tr>
					<td>
						<select style="width: #WIDTH1#px; font-weight: bold; background-color: #F4F4F4" onChange=bouquetChange() name="bouquet" size=#SELSIZE#></select>
					</td>
					<td>
						<select style="width: #WIDTH2#px; font-weight: bold; background-color: #F4F4F4" onChange=channelChange() name="channel" size=#SELSIZE#></select>
					</td>
				</tr>
			</table>
		</form>
		<table border=0 cellpadding=0 cellspacing=0>
			<tr>
				<td style="width: 200px">
					#MEPGBUTTON#
				</td>
				<td style="width: 430px" align=center>
					<a href="javascript:zapChannelBackward()"><img src="up.gif" border="0"></a>
					&nbsp;&nbsp;
					<a href="javascript:zapChannelForward()"><img src="down.gif" border="0"></a>
				</td>
			</tr>
		</table>
		<iframe name="vlccmd" src="" width="100%" height="0" frameborder="0" framemargin="0" framewidth="0"></iframe>
	</body>
</html>
