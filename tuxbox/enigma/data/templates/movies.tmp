<script language="javascript" type="text/javascript" src="dhtml.js"></script>
<script language="javascript" type="text/javascript" src="zap.js"></script>
<script language="javascript" type="text/javascript">#ZAPDATA#</script>
<script>
function update()
{
	videoData.location.reload();
	setTimeout("update()", 5000);
}
function video(key)
{
	document.location = "cgi-bin/videocontrol?command=" + key;
}
function updateVideoBar(playStatus, videoPosition, videoTime)
{
	for (var i = 1; i <= 20; i += 1)
	{
		if (i <= videoPosition)
			getElem("id", "vid" + i, null).src = "led_on.gif";
		else
			getElem("id", "vid" + i, null).src = "led_off.gif";
	}
	<!--getElem("id", "playStatus", null).firstChild.nodeValue = playStatus;-->
	getElem("id", "videoTime", null).firstChild.nodeValue = videoTime;
}
function setVideo(videoPosition)
{
	document.location = "setVideo?position=" + videoPosition;
	videoData.location.reload();
}
</script>

<map name="ctrl">
	<area shape="circle" coords="30, 25, 15" href="javascript:video('play')" alt="Play">
	<area shape="circle" coords="60, 25, 10" href="javascript:video('stop')" alt="Stop">
	<area shape="circle" coords="95, 25, 10" href="javascript:video('rewind')" alt="Rewind">
	<area shape="circle" coords="120, 25, 10" href="javascript:video('forward')" alt="Forward">
	<area shape="circle" coords="160, 25, 10" href="javascript:video('pause')" alt="Pause">
</map>
