<html>
	<head>
		<title>Enigma Web Interface - #BOX#</title>
		<link rel="stylesheet" type="text/css" href="webif.css">
		<script language="javascript" type="text/javascript" src="dhtml.js"></script>
		<script>
			function update()
			{
				parent.data.location.reload();
				window.setTimeout("parent.data.init()", 500);
				window.setTimeout("update()", parent.data.getUpdateCycleTime());
			}
			function updateTitle(title)
			{
				getElem("id", "title", null).firstChild.nodeValue = title;
			}
		</script>
	</head>
	<body id="index_big" style="margin: 0px; border: 0px; padding: 0px" background="bg.png">
		<table width="780px" align="center" height="100%" border="0" cellspacing="0" cellpadding="0">
		<tbody valign="top">
		<tr hight="134">
			<td colspan="2"><iframe name="header" width="100%" height="134px" src="header" scrolling="no" frameborder="0" marginwidth="0" marginheight="0"></iframe></td>
		</tr>
		<tr height="24">
			<td colspan="2"><iframe name="topnavi" width="100%" height="24px" src="topnavi" scrolling="no" frameborder="0" marginwidth="0" marginheight="0"></iframe></td>
		</tr>
		<tr height="100%">
			<td><iframe name="leftnavi" width="110px" height="100%" src="leftnavi" scrolling="no" frameborder="0" marginwidth="0" marginheight="0"></iframe></td>
			<td><iframe name="body" width="670px" height="100%" src="body" scrolling="auto" frameborder="0" marginwidth="0" marginheight="0"></iframe></td>
		</tr>
		</tbody>
		</table>
	</body>
</html>
