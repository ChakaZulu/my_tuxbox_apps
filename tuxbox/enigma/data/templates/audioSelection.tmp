<html>
	<head>
		<title>Audio Selection</title>
		<link rel="stylesheet" type="text/css" href="webif.css">
	</head>
	<body>
		<form name="audioselector" action="/cgi-bin/setAudio">
			<table border="0" cellpadding="0" cellspacing="10">
			<tr>
				<td>Left:</td>
				<td><input type="radio" name="channel" value="0" #0#></td>
				<td>Stereo:</td>
				<td><input type="radio" name="channel" value="1" #1#></td>
				<td>Right:</td>
				<td><input type="radio" name="channel" value="2" #2#></td>
			</tr>
			<tr>
				<td colspan="6">
					<select name="language" size="1">
						#LANGUAGES#
					</select>
				</td>
			</tr>
			<tr>
				<td colspan="6"><input type="submit" value="Apply" style="width: 100px; color:#000000"></td>
			</tr>
			</table>
		</form>
	</body>
</html>
