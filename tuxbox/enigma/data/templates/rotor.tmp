<script>
function motor(cmd)
{
	if (cmd == "goeast")
		alert("Turning east...");
	else
	if (cmd =="stepeast")
		alert("Stepping east...");
	else
	if (cmd == "gowest")
		alert("Turning west...");
	else
	if (cmd =="stepwest")
		alert("Stepping west...");
	else
	if (cmd =="stop")
		alert("Stopping...");
}
</script>
<table border="1" cellpadding="0" cellspacing="5">
	#MOTORPOSITIONS#
</table>
<h2>Diseqc 1.2 Motor Controls</h2>
<table border="0" cellpadding="0" cellspacing="10">
<tr>
<td><a href="javascript:motor('goeast')"><img src="player_rew.png" border="0"></a></td>
<td><a href="javascript:motor('stepeast')"><img src="1leftarrow.png" border="0"></a></td>
<td><a href="javascript:motor('stop')"><img src="player_stop.png" border="0"></a></td>
<td><a href="javascript:motor('stepwest')"><img src="1rightarrow.png" border="0"></a></td>
<td><a href="javascript:motor('gowest')"><img src="player_fwd.png" border="0"></a></td>
</tr>
</table>
