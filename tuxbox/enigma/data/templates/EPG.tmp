<html>
<head>
	<title>EPG</title>
	<link rel="stylesheet" type="text/css" href="/epgview.css">
	<script language="javascript">
		function record(xy)
		{
			win=window.open("/addTimerEntry?"+xy, "switchStatus", "width=200, height=200, left=0, top=0");
			win.focus();
			parent.setTimeout("reload()", 300);
		}

		function EPGDetails(xy)
		{
			win=window.open("/EPGDetails?"+xy, "switchStatus", "width=200, height=200, left=0, top=0");
			win.focus();
		}
	</script>
</head>
<body bgcolor="#ffffff"><span class="title"><b>#CHANNEL#</b></span><br>
#BODY#
</body>
</html>
