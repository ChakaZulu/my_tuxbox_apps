<form name="usbconfig">
	<table border=0 cellspacing=5 cellpadding=0>
		<tr>
			<td><input type="checkbox" name="swapusb" value="on" #SWAPUSB#></td>
			<td>Use USB Swap File:</td>
			<td><input name="swapusbfile" type="text" size="50" maxlength="50" value="#USBSWAPFILE#"></td>
		</tr>
