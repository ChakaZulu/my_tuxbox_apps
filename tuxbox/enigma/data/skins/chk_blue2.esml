			<eLabel name="description" />
		<!--	 -->
			<eLabel position="393:117" size="12:12" pixmap="button_red" name="button_red_enabled" alphatest="on" />
			<eLabel position="413:117" size="12:12" pixmap="button_green" name="button_green_enabled" alphatest="on" />
			<eLabel position="433:117" size="12:12" pixmap="button_yellow" name="button_yellow_enabled" alphatest="on" />
			<eLabel position="453:117" size="12:12" pixmap="button_blue" name="button_blue_enabled" alphatest="on" />

