<eskin>
	<author>
		<name>Christoph Ketzler</name>
		<email>chk@xplore-it.de</email>
		<title>chkdesign blue II</title>
	</author>

  <fonts>
		<font file="bluehigh.ttf" name="Blue.Regular" /> <!-- Found at dafont.com freeware -->
		<font file="bluebold.ttf" name="Blue.Bold" /> <!-- Found at dafont.com freeware -->
		<font file="v5loxicar.ttf" name="v5Robusta" /> <!-- Found at dafont.com freeware -->
		<font file="v5loxical.ttf" name="v5Lixera" /> <!-- Found at dafont.com freeware -->
	</fonts>
		
	<fontalias>
		<map name="eWindow.TitleBar" font="Blue.Bold" size="32" />
		<map name="eWindow.Childs" font="Blue.Regular" size="30" />

		<map name="eListBox.EntryText.normal" font="Blue.Regular" size="30" />
		<map name="eServiceSelector.Entry.Name" font="Blue.Regular" size="32" />
		<map name="eBouquetSelector.Entry.Name" font="Blue.Regular" size="32" />

		<map name="eventview.description" font="Blue.Regular" size="32" />
		<map name="eventview.date" font="Blue.Regular" size="30" />
		<map name="eventview.time" font="Blue.Regular" size="30" />
		<map name="eventview.channel" font="Blue.Regular" size="30" />

		<map name="lcd.menu.title" font="v5Robusta" size="18" />
		<map name="lcd.menu.element" font="v5Lixera" size="18" />

		<map name="lcd.main.service_name" font="v5Robusta" size="18" />
		<map name="lcd.main.clock" font="v5Robusta" size="18" />
	</fontalias>

	<colors>
		<color name="background" color="#ff000000" />

		<color name="header" color="#1a586D88" end="#ffffff" size="16" />
		<color name="content" color="#1aDDE1E7" end="#3F3F54" size="16" />
		<color name="content_selected" color="#CED4DB" end="#3F3F54" size="16" />
		<color name="white" color="#ffffffff" end="#ffffff" size="16" />

		<color name="osd_shadowcol" color="#11030320" />
		<color name="osd_shadow" color="#11030320" end="#B1B2D9" size="4" />
		<color name="osd_normal" color="#D0E1E7" end="#5B5A69" size="16" />
		<color name="osd_epg" color="#D0E1E7" end="#5B5A69" size="16" />

		<color name="dark" color="#586D88" end="#ffffff" size="16" />
		<color name="light" color="#CED4DB" end="#3F3F54" size="16" />
		<color name="middle" color="#ACB6C3" end="#3F3F54" size="16" />
		
		<color name="osd_timebar" color="#1010AA"/>
		<color name="osd_timebarborder" color="#ffffff"/>

		<color name="shadow_red" color="#586D88" end="#ff0303" size="16" />
		<color name="shadow_green" color="#586D88" end="#03ff03" size="16" />
		<color name="shadow_yellow" color="#586D88" end="#ffff03" size="16" />
		<color name="shadow_blue" color="#586D88" end="#0303ff" size="16" />
		<color name="shadow_gray" color="#586D88" end="#282828" size="16" />
	</colors>
	
	<colorscheme>
		<map name="global.normal.background" color="content" />
		<map name="global.normal.foreground" color="content+15" />

		<map name="global.selected.background" color="dark" />
		<map name="global.selected.foreground" color="dark+15" />

		<map name="global.disabled.background" color="content" />
		<map name="global.disabled.foreground" color="content+7" />

		<map name="eWindow.titleBar" color="header" />
		<map name="eWindow.titleBarFont" color="header+15" />

		<map name="eProgress.border" color="dark" />
		<map name="eProgress.left" color="light" />
		<map name="eProgress.right" color="middle" />
		
		<map name="eServiceSelector.highlight.background" color="dark" />
		<map name="eServiceSelector.highlight.foreground" color="dark+15" />
	</colorscheme>
	
	 <values>
	 	<value name="eWindow.titleFontSize" value="24" />
		<value name="eWindow.borderLeft" value="4" />
		<value name="eWindow.borderRight" value="4" />
		<value name="eWindow.borderBottom" value="16" />
		<value name="eWindow.borderTop" value="50" />
		<value name="eWindow.titleOffsetX" value="0" />
		<value name="eWindow.titleOffsetY" value="20" />
		<value name="eWindow.titleHeight" value="30" />
		<value name="fontsize" value="23" />
	</values>

	<images basepath="" target="fb">
		<img name="button_red" src="button_red.png" />
		<img name="button_green" src="button_green.png" />
		<img name="button_yellow" src="button_yellow.png" />
		<img name="button_blue" src="button_blue.png" />
		<img name="button_gray" src="button_gray.png" />
		<img name="osd_crypt_on" src="chk_v2/crypt_on.png" />
		<img name="osd_format_on" src="chk_v2/format_on.png" />
		<img name="osd_dolby_on" src="chk_v2/dolby_on.png" />
		<img name="osd_dolby_off" src="chk_v2/dolby_off.png" />
		<img name="osd_crypt_off" src="chk_v2/crypt_off.png" />
		<img name="osd_format_off" src="chk_v2/format_off.png" />
		<img name="br" src="chk_v2/br2.png" />
		<img name="bl" src="chk_v2/bl2.png" />
		<img name="lcd_enigma_logo" src="chk_v2/logo.png" nomerge="yes" />
		<img name="lcd_menu" src="chk_v2/lcdmenu.png" nomerge="yes" />
		<img name="lcd_scart" src="video.png" nomerge="yes" />
		<img name="speaker" src="chk_v2/speaker.png" nomerge="yes" />

		<img name="eListBox.arrow.updown" src="chk_v2/lbarrow.png" />
		<img name="eListBox.arrow.left" src="chk_v2/lbarrow_left.png" />
		<img name="eListBox.arrow.right" src="chk_v2/lbarrow_right.png" />
		<img name="eListBox.arrow.up" src="chk_v2/lbarrow_up.png" />
		<img name="eListBox.arrow.down" src="chk_v2/lbarrow_down.png" />

		<img name="eWindow.top" src="chk_v2/t.png" />
		<img name="eWindow.topLeft" src="chk_v2/tl.png" />
		<img name="eWindow.topRight" src="chk_v2/tr.png" />
		<!--<img name="eWindow.left" src="chk_v2/blind.png" />
		<img name="eWindow.right" src="chk_v2/blind.png" />-->
		<img name="eWindow.bottomLeft" src="chk_v2/bl.png" />
		<img name="eWindow.bottom" src="chk_v2/b.png" />
		<img name="eWindow.bottomRight" src="chk_v2/br.png" />
	</images>
	
	<object name="enigma_lcd_main">
		<eZapLCDMain position="0:0" size="120:64">
			<eLabel position="0:0" size="120:17" pixmap="lcd_enigma_logo" />
			<eProgress position="7:2" size="65:6" name="progress_bar" foregroundColor="0" leftColor="0" rightColor="255" border="1" />
			<eLabel position="2:17" size="118:32" font="lcd.main.service_name" wrap="on" name="service_name" />
			<eLabel position="74:49" size="46:15" font="lcd.main.clock" name="clock" align="right" />
			<eLabel position="0:53" size="11:13" pixmap="speaker" />
			<eProgress position="12:56" size="60:6" name="volume_bar" foregroundColor="255" leftColor="255" rightColor="0" border="1" />
		</eZapLCDMain>
	</object> 

	<object name="ezap_main">
		<eZapMain position="70:382" size="580:134">
                        
			<eLabel position="89:113" size="15:21" pixmap="bl" name="bl" />
			<eLabel position="565:113" size="15:21" pixmap="br" name="br" />
