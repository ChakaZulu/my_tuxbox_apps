			<eWidget position="575:0" size="5:36" backgroundColor="dark" />

			<!--Zeilen  -->
			<eWidget position="87:39" size="493:33" backgroundColor="light" />
			<eWidget position="87:76" size="493:33" backgroundColor="middle" />
			<eWidget position="158:113" size="407:21" backgroundColor="dark" />
                                      
			<!--schatten
			<eWidget position="14:85" size="113:10" backgroundColor="osd_shadowcol" />
