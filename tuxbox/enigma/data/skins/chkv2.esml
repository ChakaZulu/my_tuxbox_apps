<eskin>
	<author>
		<name>Christoph Ketzler</name>
		<email>chk@xplore-it.de</email>
	</author>
  <fonts>
		<font file="bluehigh.ttf" /> <!-- Found at dafont.com freeware -->
		<font file="bluebold.ttf" /> <!-- Found at dafont.com freeware -->
		<font file="v5loxicar.ttf" /> <!-- Found at dafont.com freeware -->
		<font file="v5loxical.ttf" /> <!-- Found at dafont.com freeware -->
		<font file="unmrs.pfa" />
		<font file="Marlett.ttf" />
	
	</fonts>

	<colors>
		<color name="background" color="#ff000000" />

		<color name="header" color="#1a586D88" end="#ffffff" size="16" />
		<color name="content" color="#1aDDE1E7" end="#3F3F54" size="16" />
		<color name="content_selected" color="#CED4DB" end="#3F3F54" size="16" />
		<color name="white" color="#ffffffff" end="#ffffff" size="16" />

		<color name="osd_shadowcol" color="#11030320" />
		<color name="osd_shadow" color="#11030320" end="#B1B2D9" size="4" />
		<color name="osd_normal" color="#D0E1E7" end="#5B5A69" size="16" />
		<color name="osd_epg" color="#D0E1E7" end="#5B5A69" size="16" />

		<color name="dark" color="#586D88" end="#ffffff" size="16" />
		<color name="light" color="#CED4DB" end="#3F3F54" size="16" />
		<color name="middle" color="#ACB6C3" end="#3F3F54" size="16" />
		
		<color name="osd_timebar" color="#1010AA"/>
		<color name="osd_timebarborder" color="#ffffff"/>


		<color name="shadow_red" color="#586D88" end="#ff0303" size="4" />
		<color name="shadow_green" color="#586D88" end="#03ff03" size="4" />
		<color name="shadow_yellow" color="#586D88" end="#ffff03" size="4" />
		<color name="shadow_blue" color="#586D88" end="#0303ff" size="4" />
		<color name="shadow_gray" color="#586D88" end="#282828" size="4" />
	</colors>
	
	<colorscheme>
		<map name="backgroundColor" color="content" />

		<map name="eServiceSelector.highlight" color="dark" />
		<map name="focusedColor" color="dark" />

		<map name="fgColor" color="content" />
		<map name="fgColorDisabled" color="content" />

		<map name="eWindow.titleBarColor" color="header" />

		<map name="eProgress.border" color="dark" />
		<map name="eProgress.left" color="light" />
		<map name="eProgress.right" color="middle" />
		
	</colorscheme>
	
	 <values>
	 	
	 	<value name="eWindow.titleFontSize" value="24" />
		<value name="eWindow.borderLeft" value="4" />
		<value name="eWindow.borderRight" value="4" />
		<value name="eWindow.borderBottom" value="16" />
		<value name="eWindow.borderTop" value="50" />
		<value name="eWindow.titleOffsetX" value="0" />
		<value name="eWindow.titleOffsetY" value="20" />
		<value name="eWindow.titleHeight" value="30" />
		<value name="fontsize" value="20" />
	</values>

	<images basepath="" target="fb">
		<img name="osd_crypt_on" src="chk_v2/crypt_on.png" />
		<img name="osd_format_on" src="chk_v2/format_on.png" />
		<img name="osd_dolby_on" src="chk_v2/dolby_on.png" />
		<img name="osd_dolby_off" src="chk_v2/dolby_off.png" />
		<img name="osd_crypt_off" src="chk_v2/crypt_off.png" />
		<img name="osd_format_off" src="chk_v2/format_off.png" />

<img name="br" src="chk_v2/br2.png" />
<img name="bl" src="chk_v2/bl2.png" />
		
		<img name="enigma_logo" src="chk_v2/logo.png" nomerge="yes" />
		<img name="lcd_menu" src="chk_v2/lcdmenu.png" nomerge="yes" />
		<img name="lcd_scart" src="video.png" nomerge="yes" />

		<img name="speaker" src="chk_v2/speaker.png" nomerge="yes" />

		
		<img name="eWindow.top" src="chk_v2/t.png" />
		<img name="eWindow.topLeft" src="chk_v2/tl.png" />
		<img name="eWindow.topRight" src="chk_v2/tr.png" />
		<!--<img name="eWindow.left" src="chk_v2/blind.png" />
		<img name="eWindow.right" src="chk_v2/blind.png" />-->
		<img name="eWindow.bottomLeft" src="chk_v2/bl.png" />
		<img name="eWindow.bottom" src="chk_v2/b.png" />
		<img name="eWindow.bottomRight" src="chk_v2/br.png" />
		
		<img name="eCheckbox.checked" src="checked.png" />
		<img name="eCheckbox.unchecked" src="unchecked.png" />
	</images>
	
	<object name="eventview">
		<eWindow cposition="50:80" csize="590:400" name="title">
			<eLabel position="10:5" size="e:e-30" name="epg_description" font="Blue Highway Regular;24" backgroundColor="content" wrap="on"/>
			<eWidget position="0:e-30" size="600:e" backgroundColor="dark" />
			<eLabel position="10:e-30" size="130:e" name="date" font="Blue Highway Regular;24" backgroundColor="dark"/>
			<eLabel position="130:e-30" size="230:e" name="time" font="Blue Highway Regular;24" backgroundColor="dark"/>
			<eLabel position="360:e-30" size="230:e" name="channel" font="Blue Highway Regular;24" backgroundColor="dark" align="right"/>
		</eWindow>
	</object>


	<object name="enigma_lcd_main">
		<eZapLCDMain position="0:0" size="120:64">
			<eLabel position="0:0" size="120:17" pixmap="enigma_logo" />
			<eLabel position="0:53" size="11:13" pixmap="speaker" />
			<eLabel position="7:17" size="113:34" font="V5 Loxica Robusta;18" wrap="on" name="service_name" />
			<eLabel position="74:49" size="46:15" font="V5 Loxica Robusta;18" name="clock" align="right" />
			<eProgress position="12:56" size="60:6" name="volume_bar" foregroundColor="255" leftColor="255" rightColor="0" border="1" />
		</eZapLCDMain>
	</object> 

	<object name="enigma_lcd_menu">
		<eZapLCDMenu position="0:0" size="120:64">
			<eLabel position="0:0" size="120:18" font="V5 Loxica Robusta;18" name="title" />
		  <eLabel position="0:18" size="120:3" pixmap="lcd_menu" />
			<eLabel position="0:22" size="120:43" font="V5 Loxica Lixera;16" name="element" wrap="on" />
			<!--eLabel position="0:61" size="120:3" pixmap="lcd_menu" /-->
		</eZapLCDMenu>
	</object>

	<object name="ezap_main">
		<eZapMain position="70:355" size="580:125">

			<eLabel position="89:101" size="15:21" pixmap="bl" name="bl" />
